{
    "manifest_version": 2,
    "name": "Frequência - Meet",
    "description": "Faz a frequência automática de quem está on-line",
    "version": "1.0",
    "content_scripts":[
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["content.js", "capture.js"]
        }
    ],
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }

}
